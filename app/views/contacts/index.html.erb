<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
  <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">List of Talent</h1>
    <div class="btn-toolbar mb-2 mb-md-0">

      <div class="mr-2">
      </div>

    </div>
  </div>

  <div class="table-responsive">
    <table class="table table-striped table-sm">
      <thead>
        <tr>
          <th>Name</th>
          <th>Type Work</th>
          <th>Seniority</th>
          <th>Location</th>
        </tr>
      </thead>
      <tbody>
      <% @contact_entries.each do |contact_entries| %>
          <tr>
            <td><%= link_to contact_entries.name, contact_path(contact_entries) %></td>
            <td><%= contact_entries.type_work %></td>
            <td><%= contact_entries.seniority %></td>
            <td><%= contact_entries.location %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</main>